{
  "name": "anima",
  "version": "2.0.0",
  "description": "CSS compiler for the Anima system",
  "scripts": {
    "build": "sass src/scss/anima.scss css/anima.css --style=expanded --no-source-map",
    "watch": "sass src/scss/anima.scss css/anima.css --style=expanded --source-map --watch",
    "generate": "node src/generate-anima-system.mjs"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "author": "Maho",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "glob": "^10.3.10",
    "inquirer": "^9.3.6",
    "prompt": "^1.3.0",
    "renamer": "^5.0.0",
    "replace": "^1.2.2",
    "sass": "^1.53.0"
  }
}
